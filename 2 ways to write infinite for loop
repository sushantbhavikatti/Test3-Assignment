package javapackage;

public class Loops {

	public static void main(String[] args) {
		
	
	 for (int i = 0; i <= 5; i++) {
	    	  
	        System.out.println(i);

				}
	 }
}

package javapackage;

public class Loops {

	public static void main(String[] args) {	
 
		int a [] = {1,2,3,4,5};
	
		for (int b:a) {
			System.out.println(b);
		}
		
	 }
}
